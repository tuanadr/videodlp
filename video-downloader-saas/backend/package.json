{
  "name": "video-downloader-saas-backend",
  "version": "1.0.0",
  "description": "Backend API for VideoDownloader SaaS",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:postgres": "node scripts/test-postgres.js",
    "db:setup": "node scripts/test-postgres.js",
    "db:migrate": "node -e \"require('./database').runMigrations()\"",
    "ffmpeg:setup": "node scripts/setup-ffmpeg.js",
    "ffmpeg:test": "node scripts/setup-ffmpeg.js",
    "streaming:setup": "npm run db:setup && npm run ffmpeg:setup",
    "generate-sitemap": "node scripts/sitemap-generator.js"
  },
  "keywords": [
    "video",
    "downloader",
    "youtube-dlp",
    "saas"
  ],
  "author": "VideoDownloader Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-compression": "^1.0.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "redis": "^4.6.6",
    "sequelize": "^6.37.7",
    "stripe": "^12.0.0",
    "winston": "^3.8.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "crypto": "^1.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@jest/globals": "^29.7.0"
  }
}
